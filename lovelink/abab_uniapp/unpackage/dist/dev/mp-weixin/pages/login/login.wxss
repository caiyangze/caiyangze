page {
  height: 100%;
  width: 100%;
  overflow: hidden;
}
.login-page {
  position: relative;
  width: 100%;
  min-height: 100vh;
  display: flex;
  flex-direction: column;
  overflow: hidden;
}
.login-page .bg-layer {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
}
.login-page .bg-layer .bg-gradient {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: linear-gradient(45deg, #12C2E9, #C471ED, #F64F59);
  background-size: 300% 300%;
  animation: gradientAnimation 15s ease infinite;
  position: relative;
}
@keyframes gradientAnimation {
0% {
    background-position: 0% 50%;
}
50% {
    background-position: 100% 50%;
}
100% {
    background-position: 0% 50%;
}
}
.login-page .bg-layer .bg-gradient::before {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: url('data:image/svg+xml;utf8,<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1440 320"><path fill="rgba(255,255,255,0.05)" fill-opacity="1" d="M0,64L48,96C96,128,192,192,288,202.7C384,213,480,171,576,165.3C672,160,768,192,864,197.3C960,203,1056,181,1152,165.3C1248,149,1344,139,1392,133.3L1440,128L1440,320L1392,320C1344,320,1248,320,1152,320C1056,320,960,320,864,320C768,320,672,320,576,320C480,320,384,320,288,320C192,320,96,320,48,320L0,320Z"></path></svg>') no-repeat bottom;
  background-size: 100% 40%;
  opacity: 0.6;
}
.login-page .bg-layer .bg-gradient::after {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: radial-gradient(circle at 20% 80%, rgba(255, 255, 255, 0.15) 0%, rgba(255, 255, 255, 0) 60%), radial-gradient(circle at 80% 20%, rgba(255, 255, 255, 0.15) 0%, rgba(255, 255, 255, 0) 60%);
}
.login-page .bg-layer .overlay-gradient {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: linear-gradient(to bottom, rgba(0, 0, 0, 0.1), rgba(0, 0, 0, 0.5));
  -webkit-backdrop-filter: blur(2px);
          backdrop-filter: blur(2px);
}
.login-page .bg-layer .overlay-gradient::before {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: url('data:image/svg+xml;utf8,<svg xmlns="http://www.w3.org/2000/svg" width="100" height="100" viewBox="0 0 100 100"><rect fill="rgba(255,255,255,0.03)" width="50" height="50" x="0" y="0"></rect><rect fill="rgba(255,255,255,0.03)" width="50" height="50" x="50" y="50"></rect></svg>');
  background-size: 20px 20px;
  opacity: 0.5;
}
.login-page .content-container {
  position: relative;
  z-index: 2;
  width: 100%;
  min-height: 100vh;
  padding: 40rpx;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
}
.login-page .content-container .header {
  margin-top: 60rpx;
  margin-bottom: 60rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.login-page .content-container .header .logo {
  width: 180rpx;
  height: 180rpx;
  border-radius: 50%;
  background-color: rgba(255, 255, 255, 0.9);
  padding: 10rpx;
  box-shadow: 0 10rpx 20rpx rgba(0, 0, 0, 0.15), 0 0 30rpx rgba(255, 255, 255, 0.2);
  position: relative;
}
.login-page .content-container .header .logo::after {
  content: "";
  position: absolute;
  top: -10rpx;
  left: -10rpx;
  right: -10rpx;
  bottom: -10rpx;
  border-radius: 50%;
  border: 2rpx solid rgba(255, 255, 255, 0.3);
  animation: pulse 2s infinite;
}
@keyframes pulse {
0% {
    transform: scale(1);
    opacity: 0.8;
}
70% {
    transform: scale(1.1);
    opacity: 0;
}
100% {
    transform: scale(1);
    opacity: 0;
}
}
.login-page .content-container .header .logo-placeholder {
  width: 100%;
  height: 100%;
  border-radius: 50%;
  background: linear-gradient(135deg, #12C2E9, #C471ED, #F64F59);
  background-size: 300% 300%;
  animation: gradientAnimation 15s ease infinite;
  display: flex;
  justify-content: center;
  align-items: center;
}
.login-page .content-container .header .logo-placeholder .logo-text {
  color: #fff;
  font-size: 32rpx;
  font-weight: bold;
  text-shadow: 0 2rpx 4rpx rgba(0, 0, 0, 0.3);
}
.login-page .content-container .header .slogan {
  margin-top: 20rpx;
  font-size: 32rpx;
  color: #fff;
  font-weight: 500;
  letter-spacing: 4rpx;
  text-shadow: 0 2rpx 4rpx rgba(0, 0, 0, 0.3);
  position: relative;
}
.login-page .content-container .header .slogan::after {
  content: "";
  position: absolute;
  bottom: -10rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 60rpx;
  height: 4rpx;
  background: linear-gradient(to right, transparent, #fff, transparent);
  border-radius: 2rpx;
}
.login-page .content-container .login-card {
  width: 100%;
  background-color: rgba(255, 255, 255, 0.95);
  border-radius: 24rpx;
  box-shadow: 0 10rpx 30rpx rgba(0, 0, 0, 0.15), 0 0 20rpx rgba(255, 255, 255, 0.1);
  padding: 40rpx 30rpx;
  margin-bottom: 40rpx;
  position: relative;
  overflow: hidden;
}
.login-page .content-container .login-card::before {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 8rpx;
  background: linear-gradient(to right, #12C2E9, #C471ED, #F64F59);
}
.login-page .content-container .login-card .tab-header {
  display: flex;
  margin-bottom: 40rpx;
  border-bottom: 2rpx solid #f0f0f0;
}
.login-page .content-container .login-card .tab-header .tab-item {
  flex: 1;
  text-align: center;
  font-size: 32rpx;
  color: #666;
  padding-bottom: 20rpx;
  position: relative;
  transition: all 0.3s ease;
}
.login-page .content-container .login-card .tab-header .tab-item.active {
  color: #C471ED;
  font-weight: bold;
}
.login-page .content-container .login-card .tab-header .tab-item.active::after {
  content: "";
  position: absolute;
  bottom: -2rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 40%;
  height: 4rpx;
  background: linear-gradient(to right, #12C2E9, #C471ED, #F64F59);
  border-radius: 4rpx;
}
.login-page .content-container .login-card .form-container .input-group {
  display: flex;
  align-items: center;
  height: 100rpx;
  border-bottom: 2rpx solid #f0f0f0;
  margin-bottom: 30rpx;
  transition: all 0.3s ease;
}
.login-page .content-container .login-card .form-container .input-group:focus-within {
  border-bottom-color: #C471ED;
}
.login-page .content-container .login-card .form-container .input-group .input-prefix {
  width: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.login-page .content-container .login-card .form-container .input-group .input-prefix .prefix-icon {
  color: #999;
  font-size: 28rpx;
}
.login-page .content-container .login-card .form-container .input-group .input-field {
  flex: 1;
  height: 100%;
  font-size: 30rpx;
  color: #333;
}
.login-page .content-container .login-card .form-container .input-group .input-suffix {
  padding-left: 20rpx;
}
.login-page .content-container .login-card .form-container .input-group .input-suffix .forget-pwd {
  color: #999;
  font-size: 26rpx;
  transition: all 0.3s ease;
}
.login-page .content-container .login-card .form-container .input-group .input-suffix .forget-pwd:active {
  color: #C471ED;
}
.login-page .content-container .login-card .form-container .input-group .input-suffix .code-btn {
  color: #C471ED;
  font-size: 26rpx;
  padding: 10rpx 0;
  transition: all 0.3s ease;
}
.login-page .content-container .login-card .form-container .input-group .input-suffix .code-btn.disabled {
  color: #999;
}
.login-page .content-container .login-card .form-container .input-group .input-suffix .code-btn:active:not(.disabled) {
  transform: scale(0.98);
  opacity: 0.8;
}
.login-page .content-container .login-card .form-container .submit-btn {
  width: 100%;
  height: 90rpx;
  background: linear-gradient(45deg, #12C2E9, #C471ED, #F64F59);
  background-size: 300% 300%;
  color: #fff;
  font-size: 32rpx;
  font-weight: bold;
  border-radius: 45rpx;
  margin-top: 40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  box-shadow: 0 10rpx 20rpx rgba(196, 113, 237, 0.4);
  animation: gradientAnimation 15s ease infinite;
}
@keyframes gradientAnimation {
0% {
    background-position: 0% 50%;
}
50% {
    background-position: 100% 50%;
}
100% {
    background-position: 0% 50%;
}
}
.login-page .content-container .login-card .form-container .submit-btn:active {
  transform: scale(0.98);
  box-shadow: 0 5rpx 10rpx rgba(233, 64, 87, 0.3);
}
.login-page .content-container .login-card .form-container .submit-btn.loading {
  opacity: 0.8;
}
.login-page .content-container .login-card .form-container .submit-btn .btn-loading {
  width: 36rpx;
  height: 36rpx;
  border: 4rpx solid rgba(255, 255, 255, 0.3);
  border-top: 4rpx solid #fff;
  border-radius: 50%;
  animation: spin 1s linear infinite;
}
@keyframes spin {
0% {
    transform: rotate(0deg);
}
100% {
    transform: rotate(360deg);
}
}
.login-page .content-container .login-card .register-link {
  text-align: center;
  margin-top: 30rpx;
  font-size: 28rpx;
  color: #666;
}
.login-page .content-container .login-card .register-link .link-text {
  color: #C471ED;
  font-weight: bold;
  transition: all 0.3s ease;
}
.login-page .content-container .login-card .register-link .link-text:active {
  opacity: 0.8;
}
.login-page .content-container .third-party-login {
  margin-top: 20rpx;
}
.login-page .content-container .third-party-login .divider {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 30rpx;
}
.login-page .content-container .third-party-login .divider .line {
  width: 80rpx;
  height: 1rpx;
  background-color: rgba(255, 255, 255, 0.5);
}
.login-page .content-container .third-party-login .divider text {
  padding: 0 20rpx;
  font-size: 24rpx;
  color: rgba(255, 255, 255, 0.8);
}
.login-page .content-container .third-party-login .social-icons {
  display: flex;
  justify-content: center;
}
.login-page .content-container .third-party-login .social-icons .social-btn {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  background-color: rgba(255, 255, 255, 0.95);
  margin: 0 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  box-shadow: 0 4rpx 10rpx rgba(0, 0, 0, 0.15), 0 0 20rpx rgba(255, 255, 255, 0.1);
  transition: all 0.3s ease;
}
.login-page .content-container .third-party-login .social-icons .social-btn:active {
  transform: scale(0.95);
  box-shadow: 0 2rpx 5rpx rgba(0, 0, 0, 0.15);
}
.login-page .content-container .third-party-login .social-icons .social-btn image {
  width: 50rpx;
  height: 50rpx;
}
.login-page .content-container .third-party-login .social-icons .social-btn .icon-placeholder {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
}
.login-page .content-container .third-party-login .social-icons .social-btn .wechat-placeholder {
  background-color: #07c160;
}
.login-page .content-container .third-party-login .social-icons .social-btn .qq-placeholder {
  background-color: #12b7f5;
}
.login-page .content-container .third-party-login .social-icons .social-btn .weibo-placeholder {
  background-color: #e6162d;
}
.login-page .content-container .agreement {
  margin-top: auto;
  padding: 40rpx 0;
  text-align: center;
  font-size: 24rpx;
  color: rgba(255, 255, 255, 0.8);
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
}
.login-page .content-container .agreement .agreement-text {
  color: rgba(255, 255, 255, 0.8);
}
.login-page .content-container .agreement .agreement-link {
  color: #fff;
  text-decoration: underline;
  transition: all 0.3s ease;
}
.login-page .content-container .agreement .agreement-link:active {
  opacity: 0.8;
}

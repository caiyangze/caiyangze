
.match-result-page.data-v-c4cdc761 {
  min-height: 100vh;
  width: 100%;
  position: relative;
  overflow: hidden;
  box-sizing: border-box;
}

/* 背景层 */
.bg-layer.data-v-c4cdc761 {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: -1;
}
.bg-gradient.data-v-c4cdc761 {
  width: 100%;
  height: 100%;
  background: linear-gradient(135deg, #ff6b9d 0%, #c44569 50%, #f8b500 100%);
}

/* 浮动爱心动画 */
.floating-hearts.data-v-c4cdc761 {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  pointer-events: none;
}
.heart.data-v-c4cdc761 {
  position: absolute;
  font-size: 24rpx;
  opacity: 0.6;
  animation: float-c4cdc761 6s ease-in-out infinite;
}
.heart-1.data-v-c4cdc761 {
  top: 20%;
  left: 10%;
  animation-delay: 0s;
}
.heart-2.data-v-c4cdc761 {
  top: 40%;
  right: 15%;
  animation-delay: 1s;
}
.heart-3.data-v-c4cdc761 {
  top: 60%;
  left: 20%;
  animation-delay: 2s;
}
.heart-4.data-v-c4cdc761 {
  top: 30%;
  left: 50%;
  animation-delay: 3s;
}
.heart-5.data-v-c4cdc761 {
  top: 70%;
  right: 25%;
  animation-delay: 4s;
}
@keyframes float-c4cdc761 {
0%, 100% {
    transform: translateY(0px) rotate(0deg);
    opacity: 0.6;
}
50% {
    transform: translateY(-20px) rotate(180deg);
    opacity: 1;
}
}

/* 内容容器 */
.content-container.data-v-c4cdc761 {
  position: relative;
  z-index: 1;
  min-height: 100vh;
  width: 100%;
  background: rgba(255, 255, 255, 0.05);
  -webkit-backdrop-filter: blur(15px);
          backdrop-filter: blur(15px);
  display: flex;
  flex-direction: column;
}

/* 导航栏 */
.nav-bar.data-v-c4cdc761 {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 30rpx;
  padding-top: calc(var(--status-bar-height, 44px) + 20rpx);
  background: rgba(255, 255, 255, 0.1);
  flex-shrink: 0;
}
.nav-left.data-v-c4cdc761 {
  width: 80rpx;
  height: 60rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.nav-icon.data-v-c4cdc761 {
  font-size: 36rpx;
  color: white;
  font-weight: bold;
}
.nav-title.data-v-c4cdc761 {
  font-size: 36rpx;
  font-weight: bold;
  color: white;
}
.nav-right.data-v-c4cdc761 {
  width: 80rpx;
}

/* 主要内容 */
.main-content.data-v-c4cdc761 {
  padding: 40rpx 0 60rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
  flex: 1;
  gap: 40rpx;
  overflow-y: auto;
  min-height: 0;
  width: 100%;
  box-sizing: border-box;
}

/* 成功区域 */
.success-section.data-v-c4cdc761 {
  text-align: center;
  flex-shrink: 0;
  margin-top: 60rpx;
  margin-bottom: 20rpx;
}
.success-icon.data-v-c4cdc761 {
  font-size: 120rpx;
  margin-bottom: 30rpx;
  animation: pulse-c4cdc761 2s ease-in-out infinite;
}
@keyframes pulse-c4cdc761 {
0%, 100% {
    transform: scale(1);
}
50% {
    transform: scale(1.1);
}
}
.success-title.data-v-c4cdc761 {
  display: block;
  font-size: 44rpx;
  font-weight: bold;
  color: white;
  margin-bottom: 15rpx;
}
.success-subtitle.data-v-c4cdc761 {
  font-size: 30rpx;
  color: rgba(255, 255, 255, 0.9);
}

/* 用户卡片 */
.user-card.data-v-c4cdc761 {
  width: calc(100% - 40rpx);
  background: rgba(255, 255, 255, 0.95);
  border-radius: 30rpx;
  padding: 50rpx 40rpx;
  box-shadow: 0 20rpx 40rpx rgba(0, 0, 0, 0.1);
  display: flex;
  flex-direction: column;
  align-items: center;
  flex-shrink: 0;
  margin: 0 20rpx;
  box-sizing: border-box;
}
.user-avatar-container.data-v-c4cdc761 {
  position: relative;
  margin-bottom: 30rpx;
}
.user-avatar.data-v-c4cdc761 {
  width: 180rpx;
  height: 180rpx;
  border-radius: 50%;
  border: 5rpx solid #ff6b9d;
}
.avatar-border.data-v-c4cdc761 {
  position: absolute;
  top: -10rpx;
  left: -10rpx;
  right: -10rpx;
  bottom: -10rpx;
  border: 4rpx solid rgba(255, 107, 157, 0.3);
  border-radius: 50%;
  animation: rotate-c4cdc761 3s linear infinite;
}
@keyframes rotate-c4cdc761 {
from {
    transform: rotate(0deg);
}
to {
    transform: rotate(360deg);
}
}
.user-info.data-v-c4cdc761 {
  text-align: center;
  margin-bottom: 30rpx;
}
.user-name.data-v-c4cdc761 {
  font-size: 38rpx;
  font-weight: bold;
  color: #333;
  margin-bottom: 15rpx;
}
.user-details.data-v-c4cdc761 {
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 20rpx;
}
.user-age.data-v-c4cdc761 {
  font-size: 28rpx;
  color: #666;
}
.user-gender.data-v-c4cdc761 {
  font-size: 32rpx;
  color: #ff6b9d;
  font-weight: bold;
}

/* 心动指数 */
.heart-rate.data-v-c4cdc761 {
  width: 100%;
  text-align: center;
}
.heart-rate-label.data-v-c4cdc761 {
  font-size: 28rpx;
  color: #666;
  margin-bottom: 20rpx;
}
.heart-rate-bar.data-v-c4cdc761 {
  width: 100%;
  height: 12rpx;
  background: #f0f0f0;
  border-radius: 6rpx;
  overflow: hidden;
  margin-bottom: 20rpx;
}
.heart-rate-fill.data-v-c4cdc761 {
  height: 100%;
  background: linear-gradient(90deg, #ff6b9d, #c44569);
  border-radius: 6rpx;
  transition: width 0.3s ease;
}
.heart-rate-value.data-v-c4cdc761 {
  font-size: 32rpx;
  font-weight: bold;
  color: #ff6b9d;
}

/* 操作按钮 */
.action-buttons.data-v-c4cdc761 {
  width: calc(100% - 40rpx);
  display: flex;
  gap: 30rpx;
  margin-bottom: 30rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
  box-sizing: border-box;
}
.action-btn.data-v-c4cdc761 {
  flex: 1;
  height: 100rpx;
  border-radius: 50rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 20rpx;
  font-weight: bold;
  transition: all 0.3s ease;
}
.action-btn.primary.data-v-c4cdc761 {
  background: linear-gradient(135deg, #ff6b9d, #c44569);
  color: white;
  box-shadow: 0 10rpx 30rpx rgba(255, 107, 157, 0.3);
}
.action-btn.secondary.data-v-c4cdc761 {
  background: rgba(255, 255, 255, 0.9);
  color: #666;
  border: 2rpx solid #e0e0e0;
}
.btn-icon.data-v-c4cdc761 {
  font-size: 32rpx;
}
.btn-text.data-v-c4cdc761 {
  font-size: 28rpx;
}

/* 再次匹配 */
.rematch-section.data-v-c4cdc761 {
  width: calc(100% - 40rpx);
  margin: 0 20rpx 40rpx;
  box-sizing: border-box;
}
.rematch-btn.data-v-c4cdc761 {
  width: 100%;
  height: 90rpx;
  background: rgba(255, 255, 255, 0.2);
  border: 2rpx solid rgba(255, 255, 255, 0.3);
  border-radius: 45rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 20rpx;
  transition: all 0.3s ease;
  -webkit-backdrop-filter: blur(10rpx);
          backdrop-filter: blur(10rpx);
}
.rematch-text.data-v-c4cdc761 {
  font-size: 28rpx;
  color: white;
}
.rematch-icon.data-v-c4cdc761 {
  font-size: 28rpx;
}

/* 响应式适配 */
@media screen and (max-height: 667px) {
.main-content.data-v-c4cdc761 {
    padding: 30rpx 20rpx 40rpx;
    gap: 30rpx;
}
.success-section.data-v-c4cdc761 {
    margin-top: 40rpx;
    margin-bottom: 15rpx;
}
.success-icon.data-v-c4cdc761 {
    font-size: 100rpx;
    margin-bottom: 20rpx;
}
.user-card.data-v-c4cdc761 {
    padding: 40rpx 30rpx;
    margin: 0 10rpx;
}
.user-avatar.data-v-c4cdc761 {
    width: 160rpx;
    height: 160rpx;
}
}

/* 安全区域适配 */
@supports (bottom: env(safe-area-inset-bottom)) {
.main-content.data-v-c4cdc761 {
    padding-bottom: calc(60rpx + env(safe-area-inset-bottom));
}
}

/* 大屏幕适配 */
@media screen and (min-width: 768px) {
.main-content.data-v-c4cdc761 {
    max-width: 750rpx;
    margin: 0 auto;
}
.user-card.data-v-c4cdc761 {
    max-width: 500rpx;
    margin: 0 auto;
}
.action-buttons.data-v-c4cdc761 {
    max-width: 500rpx;
    margin: 0 auto 30rpx;
}
.rematch-section.data-v-c4cdc761 {
    max-width: 500rpx;
    margin: 0 auto 40rpx;
}
}

/* 小屏幕适配 */
@media screen and (max-width: 375px) {
.main-content.data-v-c4cdc761 {
    padding: 40rpx 10rpx 60rpx;
}
.user-card.data-v-c4cdc761 {
    width: calc(100% - 20rpx);
    padding: 40rpx 25rpx;
    margin: 0 10rpx;
}
.action-buttons.data-v-c4cdc761 {
    width: calc(100% - 20rpx);
    gap: 20rpx;
    margin-left: 10rpx;
    margin-right: 10rpx;
}
.rematch-section.data-v-c4cdc761 {
    width: calc(100% - 20rpx);
    margin-left: 10rpx;
    margin-right: 10rpx;
}
}

